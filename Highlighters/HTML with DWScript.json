{
    "Highlighter": {
        "Info": {
            "General": {
                "MultiHighlighter": true,
                "Version": "1.0",
                "Date": "3.6.2015",
                "Sample": [
                    "<!DOCTYPE HTML>\r\n",
                    "<!-- Hello World in HTML -->\r\n",
                    "<html>\r\n",
                    "    <head>\r\n",
                    "        <title>Hello world example</title>\r\n",
                    "    </head>\r\n",
                    "    <body>\r\n",
                    "        <span>Hello, world!</span>\r\n",
                    "    </body>\r\n",
                    "</html>"
                ]
            },
            "Author": {
                "Name": "Eric Grange",
                "Mail": "eric@delphitools.info"
            }
        },
        "MainRules": {
            "Attributes": {
                "Element": "EditorGray",
				"ParentBackground": false
            },
            "SubRules": {
                "Range": [
                    {
                        "Type": "Comment",
                        "Attributes": {
                            "Element": "Comment",
                            "ParentBackground": true
                        },
                        "TokenRange": {
                            "Open": "<!--",
                            "Close": "-->"
                        }
                    },
                    {
                        "Type": "Directive",
                        "Attributes": {
                            "Element": "Directive"
                        },
                        "TokenRange": {
                            "Open": "<!",
                            "Close": ">"
                        }
                    },
					{
						"Type": "Tag",
						"Attributes": {
							"Element": "EditorWhite",
							"ParentBackground": false
						},
						"TokenRange": {
							"Open": "<?pas",
							"Close": "?>"
						},
						"SubRules": {
							"Range": [
								{
									"File": "DWScript.json",
									"IncludeRange": "DWScript"
								}
							]
						}
					},
					{
						"Type": "Tag",
						"Attributes": {
							"Element": "EditorWhite",
							"ParentBackground": false
						},
						"TokenRange": {
							"Open": "<script type=\"pascal\"",
							"Close": "/script>"
						},
						"SubRules": {
							"Range": [
								{
									"File": "DWScript.json",
									"IncludeRange": "DWScript"
								}
							]
						}
					},
					{
						"Type": "Tag",
						"Attributes": {
							"Element": "EditorGreen",
							"ParentBackground": false
						},
						"Properties": {
							"AlternativeClose": ["/>"]
						},
						"TokenRange": {
							"Open": "<script",
							"Close": "/script>"
						},
						"SubRules": {
							"Range": [
								{
									"File": "JavaScript.json",
									"IncludeRange": "JavaScript"
								}
							]
						}
					},
					{
						"Name": "Style",
						"Type": "Tag",
						"Attributes": {
							"Element": "EditorBlue"
						},
						"TokenRange": {
							"Open": "<style",
							"Close": "/style>"
						},
						"SubRules": {
							"Range": [
								{
									"File": "CSS.json",
									"IncludeRange": "CSS"
								}
							]
						}
					},

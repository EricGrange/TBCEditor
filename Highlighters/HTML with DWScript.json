    "CodeFolding": {
        "Options": {
            "StringEscapeChar": "\\"
        },
        "SkipRegion": [
            {
                "File": "JavaScript.json"
            },
            {
                "File": "DWScript.json"
            }
        ],
        "FoldRegion": [
            {
                "OpenToken": "<?pas",
                "CloseToken": "?>"
            },
            {
                "File": "DWScript.json"
            },
            {
                "File": "JavaScript.json"
            }
        ]

    },
    "MatchingPair": {
        "Pairs": [
            {
                "File": "DWScript.json"
            },
            {
                "File": "JavaScript.json"
            },
            {
                "OpenToken": "<",
                "CloseToken": ">"
            }
